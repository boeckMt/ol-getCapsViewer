{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!./node_modules/**"
    ],
    "files": [
        "components/about/about.ts",
        "components/wms/helpers/eventservice.ts",
        "components/wms/helpers/startswithpipe.ts",
        "components/wms/layerdetail/layerdetail.ts",
        "components/wms/layerlist/layerlist.ts",
        "components/wms/wms.ts",
        "typings/openlayers/openlayers.d.ts",
        "typings/react/react.d.ts",
        "typings/tsd.d.ts",
        "wms-app.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
